{
    "name": "linefusion/api",
    "description": "Minimal clean PHP web framework bootstrap for LineFusion API.",
    "type": "project",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "@php -r \"file_exists('bootstrap/cache') || mkdir('bootstrap/cache', 0755, true);\""
        ],
        "serve": "@php -S localhost:8000 -t public public/index.php"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
